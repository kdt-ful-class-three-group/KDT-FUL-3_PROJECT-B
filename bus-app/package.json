  "scripts": {
    "postinstall": "electron-builder install-app-deps && electron-builder install-app-deps",
    "nxe:build:frontend": "nx build @bus-app/web",
    "nxe:build:backend": "nx build electron",
    "nxe:serve:frontend": "nx serve @bus-app/web",
    "nxe:serve:backend": "nx serve electron",
    "nxe:test:frontend": "nx test @bus-app/web",
    "nxe:test:backend": "nx test electron",
    "nxe:package:app": "nx run electron:make --prepackgeOnly",
    "nxe:make:app": "nx run electron:make",
    "server": "nx serve server",
    "web": "nx serve web",
    "build:electron": "tsc apps/electron/src/main.ts --outDir apps/electron/dist",
    "electron": "electron apps/electron/dist/main.js",
    "dev": "concurrently \"yarn server\" \"wait-on http://localhost:3333/api && yarn web\" \"wait-on http://localhost:4200 && yarn electron\""
